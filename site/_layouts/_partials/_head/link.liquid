<link rel="dns-prefetch" href="https://res.cloudinary.com">
<link rel="preconnect" href="https://res.cloudinary.com">

{% if type == "home" %}
  <link rel="alternate" type="application/rss+xml" title="RSS Feed for silvestar.codes" href="/rss.xml">
{% endif %}

{% if env.production or env.staging %}
  {% if loadCritical %}
    {% if isMentor %}
      <link rel="preload" href="/css/mentor.min.css" as="style">
    {% elsif isRibbons %}
      <link rel="preload" href="/css/ribbons.min.css" as="style">
    {% elsif isSlides %}
      <link rel="preload" href="/css/slides.min.css" as="style">
    {% elsif is360 %}
      <link rel="preload" href="/css/devcards.min.css" as="style">
    {% elsif isHexo %}
      <link rel="preload" href="/css/hexo.min.css" as="style">
    {% else %}
      <link rel="preload" href="/css/style.min.css" as="style">
    {% endif %}
  {% endif %}
{% endif %}

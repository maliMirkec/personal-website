{% if env.production or env.staging %}
  {% if loadCritical %}
    {% if isMentor %}
      <link rel="stylesheet" href="/css/mentor.min.css" media="print" onload='this.media="all"'>
    {% elsif isRibbons %}
      <link rel="stylesheet" href="/css/ribbons.min.css" media="print" onload='this.media="all"'>
    {% elsif isSlides %}
      <link rel="stylesheet" href="/css/slides.min.css" media="print" onload='this.media="all"'>
    {% elsif is360 %}
      <link rel="stylesheet" href="/css/devcards.min.css" media="print" onload='this.media="all"'>
    {% elsif isHexo %}
      <link rel="stylesheet" href="/css/hexo.min.css" media="print" onload='this.media="all"'>
    {% else %}
      <link rel="stylesheet" href="/css/style-2024.min.css" media="print" onload='this.media="all"'>
      <link rel="stylesheet" href="/css/style-2024-dark.min.css" media="(prefers-color-scheme: dark)">
    {% endif %}
  {% endif %}
{% endif %}
{% if type == 'articles-item' %}
  <link rel="stylesheet" href="/css/prism-okaidia.{% if env.production or env.staging %}min.{% endif %}css" media="print" onload='this.media="all"'>
{% endif %}

{
  "name": "silvestar.codes",
  "version": "4.0.16",
  "description": "Home of Silvestar Bistrović—a fearless web engineer, CSS developer, JAMstack enthusiast, WordPress theme specialist, author of the UI Dev Newsletter, and founder of CSS Auditors.",
  "main": "index",
  "repository": {
    "type": "git",
    "url": "https://github.com/maliMirkec/hexo-sb/"
  },
  "keywords": [
    "website",
    "maliMirkec"
  ],
  "author": "maliMirkec <malimirkeccita@gmail.com> (https://www.silvestar.codes)",
  "license": "MIT",
  "homepage": "https://www.silvestar.codes",
  "bugs": {
    "url": "https://github.com/maliMirkec/hexo-sb/issues"
  },
  "scripts": {
    "cp-redirects": "cp extras/_redirects public/_redirects",
    "cp-headers": "cp extras/_headers public/_headers",
    "cp-cv": "cp extras/silvestar-bistrovic-cv.pdf public/silvestar-bistrovic-cv.pdf",
    "cp-robots-dev": "cp extras/robots_dev.txt public/robots.txt",
    "cp-robots-prod": "cp extras/robots_prod.txt public/robots.txt",
    "cp-dev": "npm run cp-redirects & npm run cp-headers & npm run cp-cv & npm run cp-robots-dev",
    "cp-prod": "npm run cp-redirects & npm run cp-headers & npm run cp-cv & npm run cp-robots-prod",
    "rm-critical": "rm -rf assets/src/scss/critical/*.css",
    "eleventy-dev": "env ELEVENTY_ENV=development npx @11ty/eleventy --serve --quiet --watch",
    "eleventy-preview": "env ELEVENTY_ENV=preview npx @11ty/eleventy --quiet",
    "eleventy-prod": "env ELEVENTY_ENV=production npx @11ty/eleventy --serve --quiet",
    "eleventy-build": "env ELEVENTY_ENV=production npx @11ty/eleventy --quiet",
    "critical": "gulp build && npm run eleventy-prod & (wait-on http://localhost:8080/ && gulp critical)",
    "critical-rebuild": "npm run rm-critical && npm run critical",
    "dev": "killify && gulp build && (npm run eleventy-dev & gulp watch)",
    "prod": "killify && gulp build && gulp critical && (npm run eleventy-prod & gulp watch)",
    "build-dev": "gulp build && gulp critical && npm run eleventy-build && npm run cp-dev",
    "build-prod": "gulp build && gulp critical && npm run eleventy-build && npm run cp-prod"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.2",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@netlify/plugin-sitemap": "^0.8.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.5",
    "del": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "google-spreadsheet": "^3.3.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-cssimport": "^7.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-exit": "^0.0.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-include": "^2.4.1",
    "gulp-penthouse": "^0.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-stylelint": "^13.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-wait": "^0.0.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "jstransformer-markdown-it": "^2.1.0",
    "killify": "^0.0.1",
    "kss": "^3.0.1",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^8.3.1",
    "modularscale-sass": "^3.0.10",
    "normalize.css": "^8.0.1",
    "penthouse": "^2.3.3",
    "sass-mq": "5.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "uslug": "^1.0.4",
    "wait-on": "^7.0.1",
    "webpack": "^5.21.2",
    "webpack-stream": "^6.1.2"
  }
}
